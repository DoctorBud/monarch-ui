{
  "name": "monarch-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/",
    "rmdsstore": "find . -name '.DS_Store' -print -delete",
    "ui": "node_modules/.bin/vue ui",
    "serve": "node_modules/.bin/vue-cli-service serve --port 8081",
    "build": "node_modules/.bin/vue-cli-service build && cp dist/index.html dist/404.html",
    "buildandserve": "npm run build && (cd dist/ && ln -s . monarch-ui) && http-server -c-1 dist/",
    "lint": "node_modules/.bin/vue-cli-service lint",
    "test:unit": "node_modules/.bin/vue-cli-service test:unit",
    "test:e2e": "node_modules/.bin/vue-cli-service test:e2e",
    "publish": "./publish.sh"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "d3": "^5.7.0",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "genomefeaturecomponent": "^0.2.7",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.11",
    "phenogrid": "^1.4.0",
    "register-service-worker": "^1.5.2",
    "underscore": "^1.9.1",
    "vue": "^2.5.22",
    "vue-form-wizard": "^0.8.4",
    "vue-json-tree": "^0.3.3",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli": "^3.3.0",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-mocha": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "chai": "^4.2.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.15.0",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-bootstrap-vue": "^0.1.0",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.22"
  }
