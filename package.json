    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "register-service-worker": "^1.5.2",
    "underscore": "^1.9.1",
    "vue": "^2.5.17",
    "vue-form-wizard": "^0.8.4",
    "vue-json-tree": "^0.3.3",
    "vue-router": "^3.0.1"
